<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>

<AnchorPane fx:id="musicListPane" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.durex.music.controller.MusicListController">
   <children>
      <TableView id="music-table" fx:id="musicListTable" prefWidth="730.0" stylesheets="@../css/music-list.css" AnchorPane.topAnchor="0.0" StackPane.alignment="CENTER_LEFT">
        <columns>
            <TableColumn fx:id="id" maxWidth="50.0" minWidth="50.0" prefWidth="50.0" text="id" visible="false" />
          <TableColumn fx:id="name" maxWidth="313.0" minWidth="313.0" prefWidth="313.0" text="歌曲" />
          <TableColumn fx:id="singer" maxWidth="168.0" minWidth="168.0" prefWidth="168.0" text="歌手" />
            <TableColumn fx:id="album" maxWidth="175.0" minWidth="175.0" prefWidth="175.0" text="专辑" />
            <TableColumn fx:id="duration" maxWidth="70.0" minWidth="70.0" prefWidth="70.0" text="时长" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</AnchorPane>
