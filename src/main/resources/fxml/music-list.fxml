<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>

<AnchorPane fx:id="musicListPane" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.durex.music.controller.MusicListController">
   <children>
      <TableView id="music-table" fx:id="musicListTable" stylesheets="@../css/music-list.css" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0" StackPane.alignment="CENTER_LEFT">
        <columns>
          <TableColumn fx:id="name" maxWidth="-1.0" minWidth="-1.0" prefWidth="-1.0" text="歌曲" />
          <TableColumn fx:id="singer" maxWidth="-1.0" minWidth="-1.0" prefWidth="-1.0" text="歌手" />
            <TableColumn fx:id="album" maxWidth="-1.0" minWidth="-1.0" prefWidth="-1.0" text="专辑" />
            <TableColumn fx:id="duration" maxWidth="-1.0" minWidth="-1.0" prefWidth="-1.0" text="时长" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</AnchorPane>
